<!doctype html>
<html {{ site.language_attributes }} class="no-js">
<head>
  <meta charset="{{ site.charset }}">
  <title>
    {% if wp_title %}
      {{ wp_title }} - {{ site.name }}
    {% else %}
      {{ site.name }}
    {% endif %}
  </title>
  <meta name="description" content="{{ site.description }}">

  <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta name="theme-color" content="#012A36">
  <link rel="pingback" href="{{ site.pingback_url }}">
  <link rel="preload" href="{{ site.theme.link }}/assets/js/manifest.js" as="script">
  <link rel="preload" href="{{ site.theme.link }}/assets/js/vendor.js" as="script">
  <link rel="preload" href="{{ site.theme.link }}/assets/js/main.js" as="script">
  <link rel="preload" href="{{ site.theme.link }}/assets/css/main.css" as="style">
  
  {{ function('wp_head') }}
