<header class="c-header">
  {% block header %}
    <div class="c-header__content o-wrapper">
      <h1 class="c-header__logo" role="banner">
        <a class="c-header__logo-link" href="{{site.url}}" rel="home">{{site.name}}</a>
      </h1>

      <nav class="c-header__nav c-nav--primary" role="navigation">
        {% include 'menu.twig' with {'menu': menu.get_items} %}
      </nav>
    </div>
  {% endblock %}
</header>
